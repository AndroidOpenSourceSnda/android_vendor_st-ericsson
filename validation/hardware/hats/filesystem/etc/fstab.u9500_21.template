# /etc/fstab: static file system information.
#
# <file system> <mount point>   	<type>  <options>       		<dump>  <pass>
proc		/proc			proc	defaults			0	0
sysfs		/sys			sysfs	defaults			0	0
debugfs		/sys/kernel/debug	debugfs defaults			0	0
tmpfs		/tmp			tmpfs	defaults			0	0
/dev/mmcblk0p2	/system		auto	sync,dirsync,noatime,nodiratime	0	0
/dev/mmcblk1p1	/mnt/UserFS1		ext2	noauto				0	0
/dev/mmcblk1p2	/mnt/UserFS2		vfat	noauto				0	0
/dev/mmcblk2p1	/mnt/SDCard		vfat	noauto				0	0
/dev/sda1	/mnt/UsbHost		vfat	noauto				0	0
/dev/mmcblk0p4	/data			auto	sync,dirsync,noatime,nodiratime	0	0

