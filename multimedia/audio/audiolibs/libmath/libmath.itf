#include <audiolibs/common/include/mathematics.h>

interface audiolibs.libmath.libmath unmanaged
{
    short sqrt16           (short x);
    void  sqrt16_float     (short *p_y, int *p_right_shift);
    long  sqrt32           (long  x);
    void  sqrt32_float     (long  *p_y, int *p_right_shift);
    short sqrt16_fast      (short x);
    void  sqrt16_float_fast(short *p_y, int *p_right_shift);
    long  sqrt32_fast      (long  x);
    void  sqrt32_float_fast(long  *p_y, int *p_right_shift);
}

