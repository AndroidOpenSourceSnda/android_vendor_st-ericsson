armcc -g -c --cpu=Cortex-A9 -Otime -O3 --asm --interleave --exceptions --dwarf3 --fpu=SoftVFP+VFPv3 --no_unaligned_access --arm_linux --min_array_alignment=8 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ mdrc5b_limiter_CA9.c -o mdrc5b_limiter_CA9_arm_linux.o
armcc -g -c --cpu=Cortex-A9 -Otime -O3 --asm --interleave --exceptions --dwarf3 --fpu=SoftVFP+VFPv3 --no_unaligned_access --arm_linux --min_array_alignment=8 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ mdrc5b_filters.c -o mdrc5b_filters_CA9_arm_linux.o
armcc -g -c --cpu=Cortex-A9 -Otime -O3                    --exceptions --dwarf3 --fpu=SoftVFP+VFPv3 --no_unaligned_access --arm_linux --min_array_alignment=8 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/debug_samples.c -o debug_samples_CA9_arm_linux.o
arm-linux-g++ -g -c -fno-rtti -march=armv7-a -mtune=cortex-a8 -mfloat-abi=softfp -mfpu=neon -pthread -fpic -Wall -fno-exceptions -fno-strict-aliasing -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/Profile.c -o Profile_CA9_arm_linux.o
arm-linux-g++ -g -c -fno-rtti -march=armv7-a -mtune=cortex-a8 -mfloat-abi=softfp -mfpu=neon -pthread -fpic -Wall -fno-exceptions -fno-strict-aliasing -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/get_param.c -o get_param_CA9_arm_linux.o
arm-linux-g++ -g -c -fno-rtti -march=armv7-a -mtune=cortex-a8 -mfloat-abi=softfp -mfpu=neon -pthread -fpic -Wall -fno-exceptions -fno-strict-aliasing -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/standalone_mdrc5b.c -o standalone_mdrc5b_CA9_arm_linux.o
arm-linux-g++                 -march=armv7-a -mtune=cortex-a8 -mfloat-abi=softfp -mfpu=neon -pthread -fpic -Wall -fno-exceptions -fno-strict-aliasing mdrc5b_util_CA9_arm_linux.o mdrc5b_limiter_CA9_arm_linux.o mdrc5b_compressor_CA9_arm_linux.o mdrc5b_CA9_arm_linux.o mdrc5b_filters_CA9_arm_linux.o debug_samples_CA9_arm_linux.o get_param_CA9_arm_linux.o Profile_CA9_arm_linux.o standalone_mdrc5b_CA9_arm_linux.o -lm -o mdrc5b_CA9_linux

