#!/bin/sh
echo "Settings for Audio USB Headset"

echo  "Before launching this script, make sure that usb Headset is plugged"

#MIC_CTRL & VIDEO_CTRL configuration

sk-ab w 0x1001 0x9E
sk-ab w 0x1011 0x02
sk-ab w 0x1021 0x02
sk-ab w 0x1004 0x7A
sk-ab w 0x1014 0x16
sk-ab w 0x1024 0x14

#USB HEADSET configuration

sk-ab w 0x0BA2 0x0C
sk-ab w 0x0382 0x03
sk-ab w 0x0d05 0x87
sk-ab w 0x0d74 0x03
sk-ab w 0x0d75 0x1F
sk-ab w 0x0383 0x0A
sk-ab w 0x0d33 0x09

#USB HEADSET gains

sk-ab w 0x0d14 0x0A
sk-ab w 0x0d43 0x1F
sk-ab w 0x0d47 0x00
sk-ab w 0x0d48 0x00
sk-ab w 0x0d4F 0x08
sk-ab w 0x0d50 0x08
sk-ab w 0x0d73 0x22

echo "Done"


