<?xml version="1.0" encoding="UTF-8" ?>
<profile layout="grid" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ui_validator.xsd">

	<section name="Control_Keys" extendable="false" columns="1">
	
	
		<record name="Control_Keys" match="16" label="Control_Keys" minCount="1" maxCount="1" columns="1">
			
			<attribute name="Network Lock Key">
				<visual label="Network Lock Key" type="TEXT_FIELD" regexpValidator="[0-9]{8,16}"/>
			</attribute>
			
			<attribute name="Network Subset Lock Key">
				<visual label="Network Subset Lock Key" type="TEXT_FIELD" regexpValidator="[0-9]{8,16}"/>
			</attribute>
			
			<attribute name="Service Provider Lock Key">
				<visual label="Service Provider Lock Key" type="TEXT_FIELD" regexpValidator="[0-9]{8,16}"/>
			</attribute>
			
			<attribute name="Corporate Lock Key">
				<visual label="Corporate Lock Key" type="TEXT_FIELD" regexpValidator="[0-9]{8,16}"/>
			</attribute>
			
			<attribute name="Flexible ESL Lock Key">
				<visual label="Flexible ESL Lock Key" type="TEXT_FIELD" regexpValidator="[0-9]{8,16}"/>
			</attribute>
			
			<attribute name="Fill with" >
				<visual label="Fill with" type="COMBO" selected="false">
					<option label="0" value="0"/>
					<option label="1" value="1"/>
					<option label="2" value="2"/>
					<option label="3" value="3"/>
					<option label="4" value="4"/>
					<option label="5" value="5"/>
					<option label="6" value="6"/>
					<option label="7" value="7"/>
					<option label="8" value="8"/>
					<option label="9" value="9"/>
					<option label="Random" value="Random"/>
					
    			</visual>									
			</attribute>
			
			
		</record>
		
			        		        
	</section>

	<section name="Customization" extendable="false" columns="1">
	
	<record name="IMEI_in_flash" match="1" label="IMEI in flash" minCount="1" maxCount="1">
			<attribute name="IMEI_in_flash">
				<visual label="IMEI in flash" type="TEXT_FIELD" regexpValidator="[0-9]{14}"/>
			</attribute>
		</record>
		
		<record name="Default_Data" match="256" label="Default Data" minCount="1" maxCount="1">
			<attribute name="Default_Data">
				<visual label="Default Data" type="PATH" >
				<option label="Default Data File" value= "*.csd"/> 
				
				</visual>
			</attribute>
		
		</record>
	
		
		

		
		
	</section>

</profile>