armcc -g -c --cpu=Cortex-A9 -Otime -O3 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/get_param.c -o get_param_CA9_arm_simu.o
armcc -g -c --cpu=Cortex-A9 -Otime -O3 -UMDRC5B_DEBUG -DARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ ../standalone/standalone_mdrc5b.c -o standalone_mdrc5b_CA9_arm_simu.o
armlink --cpu=Cortex-A9 --scatter scatter_CA9.scat --entry 0x2000 --map --symbols --info sizes,totals Init_CA9.o InitCache_CA9.o Retarget_CA9.o Benchmark_CA9.o mdrc5b_util_CA9_arm_simu.o mdrc5b_limiter_CA9_arm_simu.o mdrc5b_compressor_CA9_arm_simu.o mdrc5b_CA9_arm_simu.o mdrc5b_filters_CA9_arm_simu.o debug_samples_CA9_arm_simu.o get_param_CA9_arm_simu.o standalone_mdrc5b_CA9_arm_simu.o ../../../tools/ARM/perf/target/lib/cortexA9-x-rvct-x/libtperf.a ../../../tools/ARM/pmu/lib/cortexA9-x-rvct-x/libpmu.a -o mdrc5b_CA9_simu.axf > mdrc5b_CA9_simu.map

armcc -g -c --cpu=Cortex-A9 -Otime -O3 --asm --interleave --exceptions --dwarf3 --fpu=SoftVFP+VFPv3 --no_unaligned_access --arm_linux --min_array_alignment=8 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ mdrc5b.c -o mdrc5b_CA9_arm_linux.o
armcc -g -c --cpu=Cortex-A9 -Otime -O3 --asm --interleave --exceptions --dwarf3 --fpu=SoftVFP+VFPv3 --no_unaligned_access --arm_linux --min_array_alignment=8 -UMDRC5B_DEBUG -UARM_CYCLES -UMEASURE_CYCLES -I../include -I../../../audiolibs/common/include -I../../../audiolibs/fake_dsptools/ mdrc5b_util_CA9.c -o mdrc5b_util_CA9_arm_linux.o
