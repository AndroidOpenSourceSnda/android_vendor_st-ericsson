# Mount point	fs	device
/system		ext4	/dev/block/mmcblk0p2
/cache		ext4	/dev/block/mmcblk0p4
/data		ext4	/dev/block/mmcblk0p5
/sdcard		vfat	/dev/block/mmcblk0p7
/misc		emmc	/dev/block/mmcblk0p6
/boot		toc	/dev/block/mmcblk0.BOOT
/recovery	toc	/dev/block/mmcblk0.RECOVERY
/modem		toc	/dev/block/mmcblk0.MODEM
/modemfs	toc	/dev/block/mmcblk0.MODEMFS
