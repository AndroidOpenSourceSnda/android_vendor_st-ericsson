#!/bin/sh
case $1 in
    start) 
    echo "Starting $0..."
#If kernel does not support VT then inodes are not generated by mdev.
#Therefore create some fake VT to ensure that busybox won't complain.
    for i in 0 1 2 3 4 5;
      do
      [ -e /dev/tty$i ] || ln -s null /dev/tty$i ;
    done
    ;;
    stop) 	
    echo "Stopping $0..."
    echo "Nothing to be done"
    ;;
esac
exit
