MMROOT          :=$(LBP_BUILD_TOP)/vendor/st-ericsson/multimedia
export MMROOT
MMBUILDOUT      :=$(LBP_BUILD_TOP)/out/ux600_svp/obj/mmbuildout
export MMBUILDOUT
MM_MAKEFILES_DIR:=$(MMBUILDOUT)/mkfiles
export MM_MAKEFILES_DIR
PLATFORM        :=svp9600_v1-linux
export PLATFORM
TARGET_CROSS_COMPILE:=arm-none-linux-gnueabi-
export TARGET_CROSS_COMPILE
PATH :=$(LBP_BUILD_TOP)/toolchain/arm-2009q3/bin:$(PATH)
export PATH

#a verifier
TOOLS_PATH     :=$(LBP_BUILD_TOP)/vendor/st-ericsson/tool
export TOOLS_PATH    #pas sur
STE_SOFTWARE   :=lbp
export STE_SOFTWARE
SW_VARIANT      :=android
export SW_VARIANT

MAKEDIR=..

clean all lib solib %:
	@echo
	@echo -----> Call make for dir '$(MAKEDIR)' with command '$(MAKECMDGOALS)'
	@echo
	make -C $(MAKEDIR) $*
	
