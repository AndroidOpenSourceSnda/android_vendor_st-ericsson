#Sets video save resolution

alias RESOLUTION $1

if RESOLUTION == 0
	alias VIDEO_SAVE_RESOLUTION_LR 		OMX_TRUE
	alias VIDEO_SAVE_RESOLUTION_HR 		OMX_FALSE
else
	if RESOLUTION == 1
		alias VIDEO_SAVE_RESOLUTION_LR 		OMX_FALSE
		alias VIDEO_SAVE_RESOLUTION_HR 		OMX_TRUE
	else
		alias VIDEO_SAVE_RESOLUTION_LR 		OMX_TRUE
		alias VIDEO_SAVE_RESOLUTION_HR 		OMX_TRUE
	endif
endif